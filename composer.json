{
    "name": "selwyntcy/omnipay-cuhkjetco",
    "description": "Omnipay driver for CUHK Jetco payment gateway",
    "keywords": [
        "omnipay",
        "cuhkjetco"
    ],
    "homepage": "https://github.com/selwyntcy/omnipay-cuhkjetco",
    "license": "MIT",
    "authors": [
        {
            "name": "Selwyn Tang",
            "email": "selwyntcy@gmail.com",
            "homepage": "https://github.com/selwyntcy"
        }
    ],
    "require": {
        "omnipay/common": "~3.0"
    },
    "require-dev": {
        "omnipay/tests": "~3.0",
        "squizlabs/php_codesniffer": "^3",
        "phpro/grumphp": "^0.14"
    },
    "autoload": {
        "psr-4": {
            "Omnipay\\CUHKJetco\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "League\\CUHKJetco\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 src/",
        "fix-style": "phpcbf -p --standard=PSR2 src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
